#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/dist/bin/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/dist/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/dist/bin/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/dist/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules/fastmcp/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/fastmcp@1.20.5/node_modules:/Users/mark.han/Documents/my/study.mcp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fastmcp/dist/bin/fastmcp.js" "$@"
else
  exec node  "$basedir/../fastmcp/dist/bin/fastmcp.js" "$@"
fi
